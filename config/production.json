{
    "server": {
        "domain": "https://github.com/JEBoothjr",
        "static": "./server/public"
    },
    "db": {
        "hosts": ["localhost"],
        "port": 9042
    },
    "logging": {
        "winston": {
            "transports": [{
                "name": "Console",
                "options": {
                    "level": "debug",
                    "silent": false,
                    "colorize": false,
                    "timestamp": false
                }
            }, {
                "name": "DailyRotateFile",
                "options": {
                    "level": "debug",
                    "silent": true,
                    "colorize": false,
                    "timestamp": true,
                    "filename": "./logs/floqast-",
                    "datePattern": "yyyy-MM-dd.log",
                    "maxsize": "1048576",
                    "json": true
                }
            }]
        }
    }
}